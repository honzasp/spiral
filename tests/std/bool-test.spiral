(program
  (import std)
  (import std.bool)
  (import std.test)

  (test "true" (lambda (t)
    (assert t true)))
  (test "false" (lambda (t)
    (assert-not t false)))
  (test "not" (lambda (t)
    (assert-not t (not true))
    (assert-not t (not 0))
    (assert t (not false))
    (assert-eqv t (not false) true)))
  (test "xor" (lambda (t)
    (assert t (xor true false))
    (assert t (xor false true))
    (assert-not t (xor true true))
    (assert-not t (xor false false)))))

(program
  (fun alloc-use-vec (depth)
    (var saved-vec (vec-make 100))
    (vec-set! saved-vec 4 depth)
    (alloc-drop-vec depth)
    (== depth (vec-get saved-vec 4)))
  (fun alloc-drop-vec (depth)
    (vec-make 100)
    (if (> depth 0) (alloc-use-vec (- depth 1)) true))
  (println (alloc-drop-vec 1000)))

(program
  (import std)

  (var forty-two 42)
  (fun add-forty-two (x)
    (+ x forty-two))
  (fun sub-forty-two (x)
    (- x forty-two))
  (fun add-sub-forty-two (x)
    (add-forty-two (sub-forty-two x)))
  (println (add-forty-two 8))
  (println (add-sub-forty-two 30))

  (var the-vector (vec-make 10))
  (var set-the-vec! (lambda (idx val)
    (vec-set! the-vector idx val)))
  (set-the-vec! 2 1000)
  (set-the-vec! 3 2000)
  (println (vec-get the-vector 3)))
